{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "framework": "nextjs",
    "buildCommand": "npm run build",
    "installCommand": "npm install",
    "outputDirectory": ".next",
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "https://visasight-backend.up.railway.app/api/:path*"
        },
        {
            "source": "/health",
            "destination": "https://visasight-backend.up.railway.app/health"
        }
    ]
}